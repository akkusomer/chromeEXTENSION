{
  "manifest_version": 3,
  "name": "Captcha Solver",
  "version": "1.0",
  "description": "Şirket Captcha otomatik çözme sistemi",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://hks.hal.gov.tr/*"],
      "js": ["content.js"]
    }
  ]
}
